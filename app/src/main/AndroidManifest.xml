<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mfikrihasani.imagerecognition">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name="." /> -->
        <activity android:name=".AsciRecognitor" />
        <activity android:name=".ThinningActivity" />
        <activity android:name=".HistogramSpecification" />
        <activity android:name=".HistogramEqualization" />
        <activity android:name=".ImageSmoothing" />
        <activity android:name=".Histogram" />
        <activity android:name=".FaceDetection" />
        <activity android:name=".EdgeDetectection" />
        <activity android:name=".ArialNumberRecog" />
        <activity android:name=".DiscreteFourierTransform"></activity>
    </application>

</manifest>